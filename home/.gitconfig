[user]
	name = Mike Hanby
	email = mhanby@uab.edu
[github]
	user = flakrat
	email = flakrat@yahoo.com
[core]
  autocrlf = false
  safecrlf = true
[alias]
  co = checkout
  ci = commit
  st = status
  br = branch
  lg = "log --pretty=format:'%C(yellow)%h%C(reset) %C(blue)%an%C(reset) %C(cyan)%cr%C(reset) %s %C(green)%d%C(reset)' --graph --date-order"
  fa = "fetch --all"
  type = cat-file -t
  dump = cat-file -p
  last = log -1 HEAD
[color]
  ui = auto
[color "branch"]
  current = yellow reverse
  local = yellow
  remote = green
[color "diff"]
  meta = yellow bold
  frag = magenta bold
  old = red bold
  new = green bold
[color "status"]
  added = green
  changed = yellow
  untracked = red
